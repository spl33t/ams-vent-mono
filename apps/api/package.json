{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "env:dev": "dotenvx run -f .env.development -f .env --",
    "env:prod": "dotenvx run -f .env.production -f .env --",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run env:dev -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npm run env:prod -- npm run prisma:m-d && npm run env:prod -- node dist/main",
    "prisma:dev:m-d": "npm run env:dev -- prisma migrate dev",
    "prisma:dev:p": "npm run env:dev -- prisma db push",
    "prisma:gen": "prisma generate",
    "prisma:m-d": "npm run env:prod -- prisma migrate deploy",
    "prisma:p": "npm run env:prod -- prisma db push",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.6.4",
    "@nestia/core": "^3.9.0",
    "@nestia/e2e": "^0.7.0",
    "@nestia/fetcher": "^3.9.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.17.0",
    "aws-sdk": "^2.1665.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "file-type": "^19.3.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typia": "^6.6.0"
  },
  "devDependencies": {
    "@nestia/benchmark": "^0.2.3",
    "@nestia/sdk": "^3.9.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nestia": "^5.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.17.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
